import java.util.Arrays;
public class Mathdemo {
    public int min(int a, int b) {
        if( a < b ){
            return a;
        } else {
            return b;
        }
    }
    public int max(int a, int b) {
        if( a > b ){
            return a;
        } else {
            return b;
        }
    }
public float mean(int[] args){
